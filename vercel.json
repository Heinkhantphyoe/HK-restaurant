{
  "version": 2,
  "builds": [
    {
      "src": "public/HomePage.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "Images/*",
      "use": "@vercel/static"
    }
    
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
